{
   "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "installCommand": "pip install --upgrade pip && pip install -r requirements.txt"
      }
    }
  ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "djangoTest/wsgi.py"
        }
    ]
}
